package rules;
import unice.polytech.si4.pnsinnov.teamm.drive.FileInfo

rule "mimeTypeFile" salience 0
when
    $file:FileInfo(acceptedMimeType)
then
    $file.moveFile("mimetype");
end

rule "extensionFile" salience 0
when
    $file:FileInfo(acceptedExtensions)
then
    $file.moveFile("extension");
end

rule "default" salience -1001
when
    $file:FileInfo()
then
    $file.moveFile("_NoRuleApplied");
end